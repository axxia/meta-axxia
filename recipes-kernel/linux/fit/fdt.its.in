/*
 * LSI Axxia FIT image for U-Boot (device tree only).
 */

/dts-v1/;

/ {
	description = DESCRIPTION;
	#address-cells = <1>;

	images {
		fdt@1 {
			description = "Flattened Device Tree blob";
			data = /incbin/("./linux.dtb");
			type = "flat_dt";
			arch = "ARCH";
			compression = "none";
			hash@1 {
				algo = "crc32";
			};
			hash@2 {
				algo = "sha1";
			};
		};
	};

	configurations {
		default = "conf@1";
		conf@1 {
			description = DESCRIPTION;
			fdt = "fdt@1";
		};
	};
};
